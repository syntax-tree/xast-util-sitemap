{
  "name": "xast-util-sitemap",
  "version": "1.3.0",
  "description": "xast utility to build a sitemap",
  "license": "MIT",
  "keywords": [
    "unist",
    "xast",
    "xast-util",
    "util",
    "utility",
    "xml",
    "sitemap"
  ],
  "repository": "syntax-tree/xast-util-sitemap",
  "bugs": "https://github.com/syntax-tree/xast-util-sitemap/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/xast": "^1.0.0",
    "bcp-47-normalize": "^2.0.0",
    "unist-builder": "^3.0.0",
    "xastscript": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "c8": "^7.0.0",
    "concat-stream": "^2.0.0",
    "prettier": "^2.0.0",
    "regenerate": "^1.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xast-util-to-xml": "^3.0.0",
    "xo": "^0.53.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "complexity": "off",
      "max-depth": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true
  }
}
